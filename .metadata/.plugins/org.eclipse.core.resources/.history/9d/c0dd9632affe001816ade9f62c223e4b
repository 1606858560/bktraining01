import java.util.Arrays;

public class Array {
	public static void main(String args[]) {

//		int[] arr = new int[]{1,2,3,4,5};
		int[] arr = new int[5];
		arr[0]=1;
		arr[1]=2;
		arr[2]=3;
		arr[3]=4;
		arr[4]=5;

		for (int element: arr) {
            System.out.println(element);
        }
		
		for(int arr1=0; arr1<5; arr1++) {
			System.out.println(arr1);
		}
		
		//System.out.println(Arrays.toString(arr));	
	}
	
}
