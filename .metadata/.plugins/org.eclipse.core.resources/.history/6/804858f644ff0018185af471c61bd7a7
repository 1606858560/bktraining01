import java.util.Arrays;

public class ArrayAdd {
	public static void main(String args[]) {

//		int[] arr = new int[]{1,2,3,4,5};
		int[] arr = new int[3];
		int[] arr1 = new int[2];
		arr[0]=1;
		arr[1]=2;
		arr[2]=3;
		arr1[0]=4;
		arr1[1]=5;

		//for each
		int count = 0;
		for (int num : arr) {
			if(count < 3) {
				arr[count] = arr[count] + 6;
			}
			else {
				arr[count] = arr[count] + 10;
			}
            System.out.println(num);
        }
		
//		for (int num1 : arr1) {
//            num1 = num1 + 10;
//            System.out.println(num1);
//        }
//		System.out.println("");
	}
	
}
